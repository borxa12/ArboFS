<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.santirodriguezlorenzo.arbofs" >

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.WRITE_INTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/MyTheme" >
        <activity
            android:name=".activity.MainActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name=".activity.RatingActivity"
            android:label="@string/title_activity_rating" >
        </activity>
        <activity
            android:name=".activity.CalendarActivity"
            android:label="@string/title_activity_calendar" >
        </activity>
        <activity
            android:name=".activity.SplashActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activity.ContributorsActivity"
            android:label="@string/title_activity_contributors" >
        </activity>
        <activity
            android:name=".activity.SiguenosActivity"
            android:label="@string/title_activity_siguenos" >
        </activity>
        <activity
            android:name=".activity.ContactoActivity"
            android:label="@string/title_activity_contacto" >
        </activity>
        <activity
            android:name=".activity.TemplateActivity"
            android:label="@string/title_activity_template" >
        </activity>
    </application>

</manifest>
